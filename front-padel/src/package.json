    "src/**/*.js": "eslint",
    "/*.js": "eslint",
    "commands/**/*.js": "eslint",
    "mocks/**/*.js": "eslint",
    ".storybook/**/*.js": "eslint",
    "src/**/*.scss": "stylelint",
    "src/*.scss": "stylelint"
  },
  "jest-junit": {
    "outputDirectory": "coverage"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/src/**/index.js",
      "!<rootDir>/src/custom/**/*.*",
      "!<rootDir>/**/*.stories.{js,jsx,ts,tsx}",
      "!<rootDir>/src/test-utils/**/*.*"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
